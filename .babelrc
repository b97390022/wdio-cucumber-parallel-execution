{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-typescript", {
                "rewriteImportExtensions": true
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-typescript", {
                "allowDeclareFields": true
            }
        ],
        "@babel/plugin-transform-arrow-functions"
    ]
}